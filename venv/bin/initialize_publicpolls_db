#!/home/ammc/pyramid/publicpolls/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'publicpolls','console_scripts','initialize_publicpolls_db'
__requires__ = 'publicpolls'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('publicpolls', 'console_scripts', 'initialize_publicpolls_db')()
    )
